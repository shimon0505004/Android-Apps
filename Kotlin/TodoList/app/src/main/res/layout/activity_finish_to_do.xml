<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".FinishToDoActivity">

    <TextView
            android:text="@string/textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/todoNameTextView" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="119dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="176dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="177dp" android:layout_marginBottom="593dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.5"/>
    <Button
            android:text="@string/complete"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/completeButton" android:layout_marginTop="60dp"
            app:layout_constraintTop_toBottomOf="@+id/todoNameTextView" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="162dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="162dp" android:layout_marginBottom="485dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.5"/>
</android.support.constraint.ConstraintLayout>